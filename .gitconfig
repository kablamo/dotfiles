[user]
    name = Eric Johnson
    email = eric.johnson@foxtons.co.uk
[alias]
   ci = commit
   cia = commit -a
   st = status -s
   di = diff -b
   dt = difftool 
   tree = log --graph --pretty=oneline --abbrev-commit
   co = checkout
   br = branch
   mrg = merge --no-ff
[core]
   #excludesfile = "/etc/gitexcludes"
   autocrlf = input
[diff]
   tool = vimdiff
[difftool]
   prompt = 1
